[General_S]
Meter=String
Text=General
FontFace=Roboto
FontWeight=900
AntiAlias=1
FontSize=20
FontColor=255,255,255
X=150
Y=30

[m.StartAction]
Measure=String
OnUpdateAction=[!SetOption #Set.Blur#_Shape MeterStyle "Blur_Update" ][!SetOption #Set.BlurCorner#_Shape1 MeterStyle "Blur_Update" ][!UpdateMeterGroup Update]


[Blur_Update]
Shape=Rectangle 0,0,70,25,4 | StrokeWidth 0 | Fill Color 69, 174, 19
Y=r
DynamicVariables=1
[Blur_Shape1]
Shape=Rectangle 0,0,70,25,4 | StrokeWidth 0 | Fill Color 231, 143, 54,100
Y=r
DynamicVariables=1

;========================================================================================================================================================;
;                                                                          Text                                                                          ;
;========================================================================================================================================================;

[Text_S]
Meter=String
Text=Font Size
FontFace=Roboto
AntiAlias=1
FontSize=12
FontColor=255,255,255
X=150
Y=100

[Text_Shape]
Meter=Shape
Shape=Rectangle 0,0,100,25,4 | StrokeWidth 0 | Fill Color 231, 143, 54,100
X=240r
Y=r
DynamicVariables=1
LeftMouseUpAction=[!PluginBang "MeterInput ExecuteBatch 1"]

[Text String]
Meter=String
StringAlign=Center
FontColor=255,255,255
FontFace=Roboto
FontSize=11
X=(100/2)r
Y=3r
Text=#Size1#
AntiAlias=1
StringAlign=CenterCenter

[Div]
Meter=Shape
Shape=Rectangle 0,0,340,1 | StrokeWidth 0 | Fill Color 255,255,255
DynamicVariables=1
X=150
Y=30r

