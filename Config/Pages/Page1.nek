;========================================================================================================================================================;
;                                                                        Measures                                                                        ;
;========================================================================================================================================================;

[MeterInput]
Measure=Plugin
Plugin=InputText.dll
StringAlign=Center
FontFace=Roboto
FontSize=12
X=390
Y=100
W=100
H=25
AntiAlias=1
FocusDismiss=1
SolidColor=231, 143, 54
FontColor=255,255,255
Command1=[!WriteKeyValue Variables Size1 "$UserInput$""#@#Vars.nek"][!RainmeterRefreshApp]
DefaultValue="#Size1#"
DynamicVariables=1



