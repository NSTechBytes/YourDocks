Y=140

[Color]
Meter=Shape
Shape=Rectangle 0,0,20,20,15 | StrokeWidth 0 | Fill Color #FontColor#
DynamicVariables=1
X=320r
Y=r
LeftMouseUpAction=["#@#Addons\RainRGB4.exe" "VarName=FontColor" "FileName=#@#Vars.nek"][!Refresh *]

[MeterFontColor]
Meter=String
StringAlign=Center
FontFace=Roboto
FontSize=12
X=260
Y=r
W=100
Text=
AntiAlias=1
SolidColor=#FontColor#


[Div_2]
